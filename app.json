{
  "expo": {
    "name": "Artesans Inventory APP",
    "slug": "artesans-inventory-app",
    "scheme": "artesans-inventory-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.artesans-inventory-app",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Allow Artesans Inventory APP to use Face ID to authenticate you and provide secure access to your account."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ],
      "package": "com.jasminexjin.artesansinventoryapp"
    },
    "web": {
      "bundler": "metro",
      "proxy": "http://192.168.0.14:8000"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Artesans Inventory APP to access your camera to scan product labels."
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow Artesans Inventory APP to access your Face ID biometric data."
        }
      ],
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow Artesans Inventory APP to access your Face ID biometric data."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d992c620-66e1-4bb7-af0b-d21eb91f87b6"
      }
    },
    "owner": "jasminexjin"
  }
}
